target 'SAAiOS' do
  
  use_frameworks!
  
  platform :ios, '11.0'
  
  # Pods for SAAiOS
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod "SQLite.swift"
  
  pod "Firebase/Core"
  pod "Firebase/Auth"
  pod "Firebase/Database"
  pod "FirebaseUI"
  
end

target 'SAAOSX' do
  
  use_frameworks!
  
  platform :osx, '10.13'
  
  # Pods for SAAOSX
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod 'SQLite.swift'
  pod 'FirebaseCore'
  pod 'FirebaseAuth'
  pod 'FirebaseDatabase'
  pod 'AppAuth'
  
  target 'SAAOSXTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
