source 'https://cdn.cocoapods.org/'

target 'SAAi' do
  
  use_frameworks!
  
  platform :ios, '12.0'


  # Pods for SAAi
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod "SQLite.swift"
  
end

target 'SAAo-SX' do
  
  use_frameworks!
  
  platform :osx, '10.14'
  
  # Pods for SAAOSX
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod 'SQLite.swift'
  
  target 'SAAo-SXTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
