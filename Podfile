target 'SAAiOS' do
  
  use_frameworks!
  
  platform :ios, '11.0'
  
  # Pods for SAAiOS
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod "SQLite.swift"
  
end

target 'SAAOSX' do
  
  use_frameworks!
  
  platform :osx, '10.13'
  
  # Pods for SAAOSX
  pod 'CDKSwiftOracc'
  pod 'CDKOraccInterface'
  pod 'SQLite.swift'
  
  target 'SAAOSXTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
